    <CheatEntry>
      <ID>87</ID>
      <Description>"当前选中因子 (装备/持有物页面列表加载)"</Description>
      <Options moHideChildren="1"/>
      <VariableType>Auto Assembler Script</VariableType>
      <AssemblerScript>[ENABLE]

aobscanmodule(selectedMateria,granblue_fantasy_relink.exe,48 ? ? ? 48 ? ? 74 ? 31 ? 81 ? ? ? ? ? 74 ?) // should be unique
aobscanmodule(selectedMateria_eq,granblue_fantasy_relink.exe,48 ? ? ? 8b ? 49 ? ? ? ? ? ? 41 89 84 8d ? ? ? ? ) // should be unique
alloc(selectedMateria_alloc,$1000,selectedMateria)

label(code)
label(return)

label(code_eq)
label(return_eq)

selectedMateria_alloc:
  db 00 00 00 00 00 00 00 00
code:
  mov rcx,[rdx+18]
  mov [selectedMateria_alloc], rcx
  test rcx,rcx
  jmp return
code_eq:
  mov rdx,[rbp-38]
  mov [selectedMateria_alloc], rdx
  mov eax,[rdx]
  jmp return_eq

selectedMateria:
  jmp code
  nop 2
return:

selectedMateria_eq:
  jmp code_eq
  nop
return_eq:

registersymbol(selectedMateria_alloc)
registersymbol(selectedMateria)
registersymbol(selectedMateria_eq)

[DISABLE]

selectedMateria:
  db 48 8B 4A 18 48 85 C9

selectedMateria_eq:
  db 48 8B 55 C8 8B 02

dealloc(selectedMateria_alloc)

unregistersymbol(selectedMateria_alloc)
unregistersymbol(selectedMateria)
unregistersymbol(selectedMateria_eq)
</AssemblerScript>
