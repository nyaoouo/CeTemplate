      <ID>86</ID>
      <Description>"添加因子"</Description>
      <Options moHideChildren="1"/>
      <VariableType>Auto Assembler Script</VariableType>
      <AssemblerScript>{$lua}
if syntaxcheck then return end

--                ∨∨∨∨∨∨∨∨∨∨∨∨
--                这里输入因子id
--                ∨∨∨∨∨∨∨∨∨∨∨∨
materia_to_add = "这里输入因子id" -- &lt;&lt;&lt;&lt;&lt;&lt;这里输入因子id
--                ^^^^^^^^^^
--                这里输入因子id
--                ^^^^^^^^^^



AutoAssemble([[
aobscanmodule(addMateria_pfunc_, $process, 45 ? ? 44 ? ? ? e8 * * * * 85 ? 74)
registersymbol(addMateria_pfunc_)
aobscanmodule(addMateria_this_, $process, 4c ? ? * * * * 49 83 ba ? ? ? ? ?)
registersymbol(addMateria_this_)
]])

function GetParam(ea, offset)
  local ea_ = offset + ea;
  return readInteger(ea_, true) + ea_ + 4;
end

function AddMateria()
  if (materia_to_add == "这里输入因子id") then
    ShowMessage("编辑本脚本输入id再执行")
    error()
  end
  local materia_id = type(materia_to_add) == "number" and materia_to_add or tonumber(materia_to_add, 16);
  executeCodeEx(0, nil, addMateria_pfunc, {type=0,value=readQword(addMateria_this)}, {type=0,value=materia_id}, 0, 0, 0);
end
addMateria_pfunc = GetParam(getAddress("addMateria_pfunc_"), 8);
addMateria_this = GetParam(getAddress("addMateria_this_"), 3);


[ENABLE]
AddMateria()
[DISABLE]
AddMateria()
</AssemblerScript>
