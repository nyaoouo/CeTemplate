          <ID>100</ID>
          <Description>"保存修改(按完才能持久化存档)"</Description>
          <VariableType>Auto Assembler Script</VariableType>
          <AssemblerScript>{$lua}
if syntaxcheck then return end

AutoAssemble([[
aobscanmodule(commitChanges_, $process, e8 * * * * 41 80 64 b7 ? ? )
registersymbol(commitChanges_)
]])

function GetParam(ea, offset)
  local ea_ = offset + ea;
  return readInteger(ea_, true) + ea_ + 4;
end

function Main()
  local commitChanges = GetParam(getAddress("commitChanges_"), 1);
  local selectedWeapon_alloc = getAddress("selectedWeapon_alloc");
  local p_selectedWeapon = readQword(selectedWeapon_alloc);
  for o = 0x20, 0x38, 4 do
    executeCodeEx(0, nil, commitChanges, {type=0,value=p_selectedWeapon+o});
  end
end

[ENABLE]

Main()

[DISABLE]

Main()
</AssemblerScript>
